include($KDE_DOCS/global/qt-module-defaults.qdocconf)

project = md4qt
description = Header-only C++ library for parsing Markdown.

documentationinheaders = true

headerdirs += ../md4qt
sourcedirs += ../md4qt \
              .
imagedirs += ..

defines += MD4QT_ICU_STL_SUPPORT \
           MD4QT_QT_SUPPORT

visibility = protected

navigation.landingpage  = "md4qt"
navigation.cppclassespage = "md4qt C++ Classes"

outputformats = HTML

depends += \
    qtcore

tagfile = md4qt.tags
